{
  "manifest_version": 2,
  "name": "Twitter Doge",
  "version": "1.0",
  "description": "Replaces the Twitter logo with a Doge.",
  "icons": {
    "16": "bd69_favicon16.png",
    "32": "bd69_favicon32.png",
    "48": "bd69_icon48.png",
    "128": "bd69_icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "bd69_favicon16.png",
      "32": "bd69_favicon32.png",
      "48": "bd69_icon48.png",
      "128": "bd69_icon128.png"
    }
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.twitter.com/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ]
}
